{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "esnext",
      "dom"
    ],
    "sourceMap": true,
    "declaration": true,
    "experimentalDecorators": true,
    "noImplicitReturns": true,
    "pretty": true,
    "skipLibCheck": true,
    "strict": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "composite": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true
  }
}
